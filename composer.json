{
    "name": "langgraph/langgraph-php",
    "description": "A PHP implementation of LangGraph for building language model workflows",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "langgraph",
        "llm",
        "ai",
        "workflow",
        "language-model",
        "agent",
        "chatbot"
    ],
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/your-username/langgraph-php/issues",
        "source": "https://github.com/your-username/langgraph-php"
    },
    "require": {
        "php": ">=7.4",
        "psr/container": "^1.0 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^5.0 || ^6.0",
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "LangGraph\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LangGraph\\Tests\\": "tests/",
            "LangGraph\\Examples\\": "examples/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}